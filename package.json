{
  "name": "@looksrare/sdk-v2",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "looksrare"
  ],
  "homepage": "https://looksrare.org/",
  "bugs": "https://github.com/LooksRare/sdk-v2/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/LooksRare/sdk-v2.git"
  },
  "engines": {
    "node": ">=18.14.0"
  },
  "scripts": {
    "prebuild": "rm -rf typechain dist artifacts cache",
    "prepublishOnly": "yarn build",
    "dev": "rollup -c -w",
    "build:ts": "rollup -c && tsc -d",
    "build:sc": "hardhat compile",
    "build": "yarn build:sc && yarn build:ts",
    "test": "nyc hardhat test",
    "doc": "typedoc --plugin typedoc-plugin-markdown",
    "lint": "eslint --max-warnings 0 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,yaml,yml,md}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,yaml,yml,md}'",
    "release": "release-it --only-version --set-upstream"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,yaml,yml}": "yarn format:write"
  },
  "peerDependencies": {
    "ethers": "^5.6.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@looksrare/contracts-exchange-v1": "^1.2.0",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.12.7",
    "hardhat-abi-exporter": "^2.9.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "rollup": "^2.79.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "solhint": "^3.3.7",
    "solmate": "^6.6.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typechain": "^8.0.0",
    "typedoc": "^0.23.16",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@0xsequence/multicall": "^0.41.0",
    "@looksrare/contracts-exchange-v2": "^0.1.2",
    "merkletreejs": "^0.2.32"
  }
}
